{
  "name": "dromos",
  "version": "1.0.1",
  "description": "A lib that allows to define and build routes eloquently.",
  "author": "alexandebryakin",
  "main": "./dist/cjs/index",
  "module": "./dist/esm/index",
  "types": "./dist/esm/index",
  "keywords": [
    "react",
    "react-router",
    "react-router-dom",
    "typescript"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexandebryakin/dromos.git"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run prettier && npm run lint",
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "test": "jest --config ./tests/jestconfig.json",
    "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "prettier": "prettier --write \"{src,tests}/**/*.{js,ts,jsx,tsx}\""
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.0.1",
    "jest-environment-jsdom": "^29.0.1",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.8",
    "typescript": "^4.8.2"
  }
}
